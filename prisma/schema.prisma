generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Score {
  id        Int      @id @default(autoincrement())
  username  String
  wpm       Int
  accuracy  Float @default(100.0)
  duration  Int
  createdAt DateTime @default(now())

  @@index([duration, wpm])
}
